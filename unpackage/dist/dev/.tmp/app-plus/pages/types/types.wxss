.no-exist{
	width: 280rpx;
	height: 326rpx;
	margin-top: 200rpx;
	padding-bottom: 300rpx;
}
.scroll-view_H {
	white-space: nowrap;
	width: 100%;
	-webkit-scrollbar:none;
}
.scroll-view-item {
	height: 80rpx;
	line-height: 80rpx;
	text-align: center;
	font-size: 24rpx;
}
.scroll-view-item_H {
	display: inline-block;
	width: 150rpx;
	height: 80rpx;
	line-height: 80rpx;
	text-align: center;
	font-size: 28rpx;
}
.actived-type{
	color: #F0AD4E;
}
.scroll-type{
	width:750rpx;
}
.swiper{
	width: 100%;
	height: 800rpx;
}
.swiper-item{
	width: 100%;
	display: block;
	height: 800rpx;
}
.goods-item{
	width: 620rpx;
	/* height: 0upx; */
	margin: 0 15rpx;
	padding: 30rpx 50rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-pack: start;
	-webkit-justify-content: flex-start;
	    -ms-flex-pack: start;
	        justify-content: flex-start;
	background-color: #FFFFFF;
	border-radius: 20rpx;
	margin-top: 20rpx;
}
.goods-img image{
	width: 150rpx;
	height: 200rpx;
}
.goods-content{
	margin-left: 30rpx;
	display: -webkit-box;
	display: -webkit-flex;
	display: -ms-flexbox;
	display: flex;
	-webkit-box-orient: vertical;
	-webkit-box-direction: normal;
	-webkit-flex-direction: column;
	    -ms-flex-direction: column;
	        flex-direction: column;
	-webkit-box-align: start;
	-webkit-align-items: flex-start;
	    -ms-flex-align: start;
	        align-items: flex-start;
	-webkit-justify-content: space-around;
	    -ms-flex-pack: distribute;
	        justify-content: space-around;
	font-size: 30rpx;
}
.goods-guige, .good-out{
	color: #969696;
}
.good-price{
	color: #F23031;
}

